{
	"name": "@portone/server-sdk",
	"type": "module",
	"version": "0.2.1",
	"description": "PortOne JavaScript SDK for server-side usage",
	"keywords": [
		"PortOne",
		"payment",
		"포트원",
		"결제 연동"
	],
	"author": {
		"name": "portone-dx",
		"email": "support@portone.io"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": "./src/index.ts"
	},
	"publishConfig": {
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"require": "./dist/index.cjs",
				"import": "./dist/index.mjs"
			}
		}
	},
	"scripts": {
		"build": "unbuild",
		"test:dev": "vitest --ui --typecheck",
		"test": "vitest run --typecheck",
		"prepublish": "pnpm build",
		"schema": "vite-node scripts/generate-schema.ts src/__generated__/schema.d.ts"
	},
	"devDependencies": {
		"@apidevtools/swagger-parser": "^10.1.0",
		"@expo/spawn-async": "^1.7.2",
		"@types/node": "^20.14.13",
		"@types/turndown": "^5.0.5",
		"@vitest/ui": "^2.0.5",
		"openapi-types": "^12.1.3",
		"turndown": "^7.2.0",
		"typedoc": "^0.26.5",
		"unbuild": "^2.0.0",
		"vite-node": "^2.0.5",
		"vitest": "^2.0.5"
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"license": "(Apache-2.0 OR MIT)"
}
